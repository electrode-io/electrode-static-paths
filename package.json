  },
  "repository": {
    "type": "git",
    "url": "git@github.com:electrode-io/electrode-static-paths.git"
  },
  "keywords": [
    "electrode",
    "decor",
    "static",
    "paths"
  ],
  "files": [
    "lib"
  ],
  "author": {
    "name": "Joel Chen",
    "email": "xchen@walmartlabs.com",
    "url": "https://github.com/jchip"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 8.6.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "lodash": "^4.17.11",
    "snyk": "^1.317.0"
  },
  "devDependencies": {
    "electrode-archetype-njs-module-dev": "^3.0.0",
    "@hapi/inert": "^5.2.0",
    "electrode-server": "^3.0.0",
    "superagent": "^3.8.3"
  },
  "peerDependencies": {
    "@hapi/inert": "^5.2.0"
  },
  "nyc": {
    "all": true,
